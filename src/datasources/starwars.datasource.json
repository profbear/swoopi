{
  "name": "starwars",
  "connector": "rest",
  "baseURL": "https://swapi.co/api/",
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json"
    }
  },
  "operations": [
    {
      "template": {
        "method": "GET",
        "url": "https://swapi.co/api/",
        "resourceName": ""
      },
      "functions": {
        "root": []
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://swapi.co/api/{model}/schema",
        "resourceName": ""
      },
      "functions": {
        "schema": [
          "model"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://swapi.co/api/{model}",
        "resourceName": "",
        "query": {
          "search": "{search}",
          "page": "{page}"
        }
      },
      "functions": {
        "model": [
          "model",
          "search",
          "page"
        ]
      }
    }
  ]
}
